{
  "manifest_version": 3,
  "name": "TOP",
  "version": "1.0",
  "permissions": ["nativeMessaging", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["host/install_host.bat"],
      "matches": ["<all_urls>"]
    }
  ]
}
