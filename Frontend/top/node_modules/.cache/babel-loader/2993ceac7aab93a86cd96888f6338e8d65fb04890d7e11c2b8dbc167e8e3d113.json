{"ast":null,"code":"import { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nimport { onMounted } from 'vue';\nChart.register(...registerables);\nexport default {\n  components: {\n    'bar-chart': Bar\n  },\n  setup() {\n    const updateChartData = () => {\n      const maxUsage = Math.max(...this.usageData);\n      this.chartData.datasets[0].data = this.usageData;\n      console.log(maxUsage);\n\n      // 변경할 색상들\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\n      this.chartData.datasets[0].backgroundColor = this.usageData.map(value => value === maxUsage ? maxUsageColor : defaultColor);\n      console.log(this.chartData.datasets[0].backgroundColor);\n    };\n    onMounted(updateChartData());\n  },\n  data() {\n    return {\n      usageData: [10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30],\n      chartData: {\n        labels: Array.from({\n          length: 24\n        }, (_, i) => i.toString()),\n        datasets: [{\n          label: '집중 시간(분)',\n          data: [],\n          backgroundColor: []\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        maintainAspectRatio: true,\n        scales: {\n          y: {\n            beginAtZero: true,\n            max: 60\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        }\n      }\n    };\n  }\n};","map":{"version":3,"names":["Bar","Chart","registerables","onMounted","register","components","setup","updateChartData","maxUsage","Math","max","usageData","chartData","datasets","data","console","log","maxUsageColor","defaultColor","backgroundColor","map","value","labels","Array","from","length","_","i","toString","label","chartOptions","responsive","maintainAspectRatio","scales","y","beginAtZero","plugins","legend","display"],"sources":["C:\\Users\\SSAFY\\Desktop\\S11P12A707\\Frontend\\top\\src\\components\\TimeLine.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <h2>타임라인</h2>\r\n    <bar-chart class=\"chart\" :data=\"chartData\" :options=\"chartOptions\"></bar-chart>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Bar } from 'vue-chartjs';\r\nimport { Chart, registerables } from 'chart.js';\r\nimport { onMounted } from 'vue';\r\nChart.register(...registerables);\r\n\r\nexport default {\r\n  components: {\r\n    'bar-chart': Bar\r\n  },\r\n  setup() {\r\n    const updateChartData = () => {\r\n      const maxUsage = Math.max(...this.usageData);\r\n      this.chartData.datasets[0].data = this.usageData;\r\n      console.log(maxUsage)\r\n      \r\n      // 변경할 색상들\r\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\r\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\r\n      this.chartData.datasets[0].backgroundColor = this.usageData.map(value => value === maxUsage ? maxUsageColor : defaultColor);\r\n      console.log(this.chartData.datasets[0].backgroundColor)\r\n    }\r\n    onMounted (\r\n      updateChartData()\r\n    )\r\n  },\r\n  data() {\r\n    return {\r\n      usageData: [10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30],\r\n      chartData: {\r\n        labels: Array.from({ length: 24 }, (_, i) => i.toString()),\r\n        datasets: [{\r\n          label: '집중 시간(분)',\r\n          data: [],\r\n          backgroundColor: [],\r\n        }]\r\n      },\r\n      chartOptions: {\r\n        responsive: true,\r\n        maintainAspectRatio: true,\r\n        scales: {\r\n          y: {\r\n            beginAtZero: true,\r\n            max: 60\r\n          }\r\n        },\r\n        plugins: {\r\n          legend: {\r\n            display: false\r\n          }\r\n        }\r\n      }\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  height: 300px;\r\n  width: 644px;\r\n  