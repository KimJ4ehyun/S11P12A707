{"ast":null,"code":"import { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default {\n  components: {\n    'bar-chart': {\n      extends: Bar,\n      props: ['chartData', 'options'],\n      watch: {\n        chartData() {\n          this.renderChart(this.chartData, this.options);\n        }\n      },\n      mounted() {\n        this.renderChart(this.chartData, this.options);\n      }\n    }\n  },\n  data() {\n    return {\n      usageData: [10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30],\n      chartData: {\n        labels: Array.from({\n          length: 24\n        }, (_, i) => i.toString()),\n        datasets: [{\n          label: 'Usage',\n          data: [],\n          backgroundColor: []\n        }]\n      },\n      chartOptions: {\n        scales: {\n          y: {\n            beginAtZero: true,\n            max: 60\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        }\n      }\n    };\n  },\n  mounted() {\n    this.updateChartData();\n  },\n  methods: {\n    updateChartData() {\n      const maxUsage = Math.max(...this.usageData);\n      const maxUsageColor = 'rgba(255, 0, 0, 1)'; // 빨간색\n      const defaultColor = 'rgba(211, 211, 211, 0.6)'; // 연회색\n\n      this.chartData.datasets[0].data = this.usageData;\n      this.chartData.datasets[0].backgroundColor = this.usageData.map(value => value === maxUsage ? maxUsageColor : defaultColor);\n    }\n  }\n};","map":{"version":3,"names":["Bar","Chart","registerables","register","components","extends","props","watch","chartData","renderChart","options","mounted","data","usageData","labels","Array","from","length","_","i","toString","datasets","label","backgroundColor","chartOptions","scales","y","beginAtZero","max","plugins","legend","display","updateChartData","methods","maxUsage","Math","maxUsageColor","defaultColor","map","value"]