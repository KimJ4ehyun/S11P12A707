{"ast":null,"code":"import { defineComponent, ref, onMounted } from 'vue';\nimport { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default defineComponent({\n  components: {\n    'bar-chart': {\n      extends: Bar,\n      props: ['chartData', 'options'],\n      mounted() {\n        this.renderChart(this.chartData, this.options);\n      },\n      watch: {\n        chartData() {\n          this.renderChart(this.chartData, this.options);\n        },\n        options() {\n          this.renderChart(this.chartData, this.options);\n        }\n      }\n    }\n  },\n  setup() {\n    const usageData = ref([10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30]);\n    const chartData = ref({\n      labels: Array.from({\n        length: 24\n      }, (_, i) => i.toString()),\n      datasets: [{\n        label: '집중 시간(분)',\n        data: ref([]),\n        backgroundColor: ref([])\n      }]\n    });\n    const chartOptions = ref({\n      responsive: true,\n      maintainAspectRatio: false,\n      scales: {\n        y: {\n          beginAtZero: true,\n          max: 60\n        }\n      },\n      plugins: {\n        legend: {\n          display: false\n        }\n      }\n    });\n    const updateChartData = () => {\n      const maxUsage = Math.max(...usageData.value);\n      chartData.value.datasets[0].data.value = usageData.value;\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\n      chartData.value.datasets[0].backgroundColor.value = usageData.value.map(value => value === maxUsage ? maxUsageColor : defaultColor);\n    };\n    onMounted(() => {\n      updateChartData();\n    });\n    return {\n      chartData,\n      chartOptions\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","onMounted","Bar","Chart","registerables","register","components","extends","props","mounted","renderChart","chartData","options","watch","setup","usageData","labels","Array","from","length","_","i","toString","datasets","label","data","backgroundColor","chartOptions","responsive","maintainAspectRatio","scales","y","beginAtZero","max","plugins","legend","display","updateChartData","maxUsage","Math","value","maxUsageColor","defaultColor","map"],"sources":["C:\\Users\\SSAFY\\Desktop\\S11P12A707\\Frontend\\top\\src\\components\\TimeLine.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <h2>타임라인</h2>\r\n    <bar-chart class=\"chart\" :chart-data=\"chartData\" :options=\"chartOptions\"></bar-chart>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, ref, onMounted } from 'vue';\r\nimport { Bar } from 'vue-chartjs';\r\nimport { Chart, registerables } from 'chart.js';\r\nChart.register(...registerables);\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    'bar-chart': {\r\n      extends: Bar,\r\n      props: ['chartData', 'options'],\r\n      mounted() {\r\n        this.renderChart(this.chartData, this.options);\r\n      },\r\n      watch: {\r\n        chartData() {\r\n          this.renderChart(this.chartData, this.options);\r\n        },\r\n        options() {\r\n          this.renderChart(this.chartData, this.options);\r\n        }\r\n      }\r\n    }\r\n  },\r\n  setup() {\r\n    const usageData = ref([10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30]);\r\n    const chartData = ref({\r\n      labels: Array.from({ length: 24 }, (_, i) => i.toString()),\r\n      datasets: [{\r\n        label: '집중 시간(분)',\r\n        data: ref([]),\r\n        backgroundColor: ref([]),\r\n      }]\r\n    });\r\n    const chartOptions = ref({\r\n      responsive: true,\r\n      maintainAspectRatio: false,\r\n      scales: {\r\n        y: {\r\n          beginAtZero: true,\r\n          max: 60\r\n        }\r\n      },\r\n      plugins: {\r\n        legend: {\r\n          display: false\r\n        }\r\n      }\r\n    });\r\n\r\n    const updateChartData = () => {\r\n      const maxUsage = Math.max(...usageData.value);\r\n      chartData.value.datasets[0].data.value = usageData.value;\r\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\r\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\r\n      chartData.value.datasets[0].backgroundColor.value = usageData.value.map(value => value === maxUsage ? maxUsageColor : defaultColor);\r\n    };\r\n\r\n    onMounted(() => {\r\n      updateChartData();\r\n    });\r\n\r\n    return {\r\n      chartData,\r\n      chartOptions\r\n    };\r\n  }\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  height: 200px;\r\n  width: 644px;\r\n  position: relative;\r\n}\r\n\r\n.box canvas.chart {\r\n  height: 150px !important;\r\n  width: 100% !important;\r\n  box-sizing: border-box !important;\r\n}\r\n\r\nh2 {\r\n  color: #b0bec5;\r\n  font-weight: 700;\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AAQA,SAASA,eAAe,EAAEC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACrD,SAASC,GAAE,QAAS,aAAa;AACjC,SAASC,KAAK,EAAEC,aAAY,QAAS,UAAU;AAC/CD,KAAK,CAACE,QAAQ,CAAC,GAAGD,aAAa,CAAC;AAEhC,eAAeL,eAAe,CAAC;EAC7BO,UAAU,EAAE;IACV,WAAW,EAAE;MACXC,OAAO,EAAEL,GAAG;MACZM,KAAK,EAAE,CAAC,WAAW,EAAE,SAAS,CAAC;MAC/BC,OAAOA,CAAA,EAAG;QACR,IAAI,CAACC,WAAW,CAAC,IAAI,CAACC,SAAS,EAAE,IAAI,CAACC,OAAO,CAAC;MAChD,CAAC;MACDC,KAAK,EAAE;QACLF,SAASA,CAAA,EAAG;UACV,IAAI,CAACD,WAAW,CAAC,IAAI,CAACC,SAAS,EAAE,IAAI,CAACC,OAAO,CAAC;QAChD,CAAC;QACDA,OAAOA,CAAA,EAAG;UACR,IAAI,CAACF,WAAW,CAAC,IAAI,CAACC,SAAS,EAAE,IAAI,CAACC,OAAO,CAAC;QAChD;MACF;IACF;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIf,GAAG,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC,CAAC;IACvH,MAAMW,SAAQ,GAAIX,GAAG,CAAC;MA