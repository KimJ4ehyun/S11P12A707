{"ast":null,"code":"import { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default {\n  components: {\n    'bar-chart': Bar\n  },\n  data() {\n    return {\n      usageData: [10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30],\n      chartData: {\n        labels: Array.from({\n          length: 24\n        }, (_, i) => i.toString()),\n        datasets: [{\n          label: '집중 시간(분)',\n          data: [],\n          backgroundColor: []\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        maintainAspectRatio: true,\n        scales: {\n          y: {\n            beginAtZero: true,\n            max: 60\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        }\n      }\n    };\n  },\n  mounted() {\n    this.updateChartData();\n  },\n  methods: {\n    updateChartData() {\n      const maxUsage = Math.max(...this.usageData);\n      this.chartData.datasets[0].data = this.usageData;\n      console.log(maxUsage);\n      // 변경할 색상들\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\n      this.chartData.datasets[0].backgroundColor = this.usageData.map(value => value === maxUsage ? maxUsageColor : defaultColor);\n    }\n  }\n};","map":{"version":3,"names":["Bar","Chart","registerables","register","components","data","usageData","chartData","labels","Array","from","length","_","i","toString","datasets","label","backgroundColor","chartOptions","responsive","maintainAspectRatio","scales","y","beginAtZero","max","plugins","legend","display","mounted","updateChartData","methods","maxUsage","Math","console","log","maxUsageColor","defaultColor","map","value"],"sources":["C:\\Users\\SSAFY\\Desktop\\S11P12A707\\Frontend\\top\\src\\components\\TimeLine.vue"],"sourcesContent":["<templat