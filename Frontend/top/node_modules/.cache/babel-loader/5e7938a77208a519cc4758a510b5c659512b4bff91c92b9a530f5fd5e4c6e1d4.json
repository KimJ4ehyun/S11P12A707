{"ast":null,"code":"import { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default {\n  components: {\n    'bar-chart': {\n      extends: Bar,\n      props: ['chartData', 'options'],\n      mounted() {\n        this.renderChart(this.chartData, this.options);\n      },\n      watch: {\n        chartData() {\n          this.renderChart(this.chartData, this.options);\n        },\n        options() {\n          this.renderChart(this.chartData, this.options);\n        }\n      }\n    }\n  },\n  data() {\n    return {\n      usageData: [10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30],\n      chartData: {\n        labels: Array.from({\n          length: 24\n        }, (_, i) => i.toString()),\n        datasets: [{\n          label: 'Usage',\n          data: [],\n          backgroundColor: []\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        maintainAspectRatio: false,\n        scales: {\n          y: {\n            beginAtZero: true,\n            max: 60,\n            grid: {\n              color: '#E5E5E5'\n            },\n            ticks: {\n              color: '#9e9e9e',\n              font: {\n                size: 10,\n                weight: 'bold'\n              }\n            }\n          },\n          x: {\n            grid: {\n              color: '#E5E5E5'\n            },\n            ticks: {\n              color: '#9e9e9e',\n              font: {\n                size: 10,\n                weight: 'bold'\n              }\n            }\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          },\n          tooltip: {\n            enabled: true\n          }\n        }\n      }\n    };\n  },\n  mounted() {\n    this.updateChartData();\n  },\n  methods: {\n    updateChartData() {\n      const maxUsage = Math.max(...this.usageData);\n      this.chartData.datasets[0]