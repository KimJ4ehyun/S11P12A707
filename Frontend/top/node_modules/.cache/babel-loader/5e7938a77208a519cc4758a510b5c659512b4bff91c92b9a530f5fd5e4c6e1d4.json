}, (_, i) => i.toString()),\r\n        datasets: [{\r\n          label: 'Usage',\r\n          data: [],\r\n          backgroundColor: [],\r\n        }]\r\n      },\r\n      chartOptions: {\r\n        responsive: true,\r\n        maintainAspectRatio: false,\r\n        scales: {\r\n          y: {\r\n            beginAtZero: true,\r\n            max: 60,\r\n            grid: {\r\n              color: '#E5E5E5'\r\n            },\r\n            ticks: {\r\n              color: '#9e9e9e',\r\n              font: {\r\n                size: 10,\r\n                weight: 'bold'\r\n              }\r\n            }\r\n          },\r\n          x: {\r\n            grid: {\r\n              color: '#E5E5E5'\r\n            },\r\n            ticks: {\r\n              color: '#9e9e9e',\r\n              font: {\r\n                size: 10,\r\n                weight: 'bold'\r\n              }\r\n            }\r\n          }\r\n        },\r\n        plugins: {\r\n          legend: {\r\n            display: false\r\n          },\r\n          tooltip: {\r\n            enabled: true\r\n          }\r\n        }\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.updateChartData();\r\n  },\r\n  methods: {\r\n    updateChartData() {\r\n      const maxUsage = Math.max(...this.usageData);\r\n      this.chartData.datasets[0].data = this.usageData;\r\n      // 변경할 색상들\r\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\r\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\r\n      this.chartData.datasets[0].backgroundColor = this.usageData.map(value => value === maxUsage ? maxUsageColor : defaultColor);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  height: 200px;\r\n  width: 644px;\r\n  position: relative;\r\n  padding: 10px;\r\n  background-color: #F9FAFB;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  box-sizing: border-box;\r\n}\r\n\r\n.box canvas.chart {\r\n  height: 180px !importa