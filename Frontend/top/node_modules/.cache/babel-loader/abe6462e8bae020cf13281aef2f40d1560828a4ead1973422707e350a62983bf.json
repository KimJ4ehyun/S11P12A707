{"ast":null,"code":"import { defineComponent, ref, onMounted } from 'vue';\nimport { Bar } from 'vue-chartjs';\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default defineComponent({\n  components: {\n    'bar-chart': {\n      extends: Bar,\n      props: ['chartData', 'options'],\n      mounted() {\n        this.renderChart(this.chartData, this.options);\n      },\n      watch: {\n        chartData() {\n          this.renderChart(this.chartData, this.options);\n        },\n        options() {\n          this.renderChart(this.chartData, this.options);\n        }\n      }\n    }\n  },\n  setup() {\n    const usageData = ref([10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30]);\n    const chartData = ref({\n      labels: Array.from({\n        length: 24\n      }, (_, i) => i.toString()),\n      datasets: [{\n        label: '집중 시간(분)',\n        data: [],\n        backgroundColor: []\n      }]\n    });\n    const chartOptions = ref({\n      responsive: true,\n      maintainAspectRatio: false,\n      scales: {\n        y: {\n          beginAtZero: true,\n          max: 60,\n          grid: {\n            color: '#E5E5E5'\n          },\n          ticks: {\n            color: '#9e9e9e',\n            font: {\n              size: 10,\n              weight: 'bold'\n            }\n          }\n        },\n        x: {\n          grid: {\n            color: '#E5E5E5'\n          },\n          ticks: {\n            color: '#9e9e9e',\n            font: {\n              size: 10,\n              weight: 'bold'\n            }\n          }\n        }\n      },\n      plugins: {\n        legend: {\n          display: false\n        },\n        tooltip: {\n          enabled: true\n        }\n      }\n    });\n    const updateChartData = () => {\n      const maxUsage = Math.max(...usageData.value);\n      chartData.value.datasets[0].data = usageData.value;\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\n      chartData.value.datasets[0].backgroundColor = usageData.value.map(value => value === maxUsage ? maxUsageColor : defaultColor);\n    };\n    onMounted(() => {\n      updateChartData();\n    });\n    return {\n      chartData,\n      chartOptions\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","onMounted","Bar","Chart","registerables","register","components","extends","props","mounted","renderChart","chartData","options","watch","setup","usageData","labels","Array","from","length","_","i","toString","datasets","label","data","backgroundColor","chartOptions","responsive","maintainAspectRatio","scales","y","beginAtZero","max","grid","color","ticks","font","size","weight","x","plugins","legend","display","tooltip","enabled","updateChartData","maxUsage","Math","value","maxUsageColor","defaultColor","map"],"sources":["C:\\Users\\SSAFY\\Desktop\\S11P12A707\\Frontend\\top\\src\\components\\TimeLine.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <h2>타임라인</h2>\r\n    <bar-chart class=\"chart\" :chart-data=\"chartData\" :options=\"chartOptions\"></bar-chart>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, ref, onMounted } from 'vue';\r\nimport { Bar } from 'vue-chartjs';\r\nimport { Chart, registerables } from 'chart.js';\r\nChart.register(...registerables);\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    'bar-chart': {\r\n      extends: Bar,\r\n      props: ['chartData', 'options'],\r\n      mounted() {\r\n        this.renderChart(this.chartData, this.options);\r\n      },\r\n      watch: {\r\n        chartData() {\r\n          this.renderChart(this.chartData, this.options);\r\n        },\r\n        options() {\r\n          this.renderChart(this.chartData, this.options);\r\n        }\r\n      }\r\n    }\r\n  },\r\n  setup() {\r\n    const usageData = ref([10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 30, 15, 25, 35, 10, 20, 56, 10, 20, 30]);\r\n    const chartData = ref({\r\n      labels: Array.from({ length: 24 }, (_, i) => i.toString()),\r\n      datasets: [{\r\n        label: '집중 시간(분)',\r\n        data: [],\r\n        backgroundColor: [],\r\n      }]\r\n    });\r\n    const chartOptions = ref({\r\n      responsive: true,\r\n      maintainAspectRatio: false,\r\n      scales: {\r\n        y: {\r\n          beginAtZero: true,\r\n          max: 60,\r\n          grid: {\r\n            color: '#E5E5E5'\r\n          },\r\n          ticks: {\r\n            color: '#9e9e9e',\r\n            font: {\r\n              size: 10,\r\n              weight: 'bold'\r\n            }\r\n          }\r\n        },\r\n        x: {\r\n          grid: {\r\n            color: '#E5E5E5'\r\n          },\r\n          ticks: {\r\n            color: '#9e9e9e',\r\n            font: {\r\n              size: 10,\r\n              weight: 'bold'\r\n            }\r\n          }\r\n        }\r\n      },\r\n      plugins: {\r\n        legend: {\r\n          display: false\r\n        },\r\n        tooltip: {\r\n          enabled: true\r\n        }\r\n      }\r\n    });\r\n\r\n    const updateChartData = () => {\r\n      const maxUsage = Math.max(...usageData.value);\r\n      chartData.value.datasets[0].data = usageData.value;\r\n      const maxUsageColor = 'rgba(0, 123, 255, 1)'; // 최대 사용량 막대 색상 (파란색)\r\n      const defaultColor = 'rgba(173, 216, 230, 0.6)'; // 기본 막대 색상 (연한 파란색)\r\n      chartData.value.datasets[0].backgroundColor = usageData.value.map(value => value === maxUsage ? maxUsageColor : defaultColor);\r\n    };\r\n\r\n    onMounted(() => {\r\n      updateChartData();\r\n    });\r\n\r\n    return {\r\n      chartData,\r\n      chartOptions\r\n    };\r\n  }\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  height: 200px;\r\n  width: 644px;\r\n  position: relative;\r\n  padding: 10px;\r\n  background-color: #F9FAFB;\r\n  border-radius: 10px;\r\n  box